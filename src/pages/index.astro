---
import Typst from "../lib/Typst.astro";

const code = `
#let a = 114
#let b = 514
#(str(a) + str(b))
`
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<body>
		<h1>Astro</h1>
		<main>
			<Typst code={code} style={true}></Typst>
		</main>
	</body>
</html>
